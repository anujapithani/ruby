<h1>Student Registration form</h1>
    <%= form_with scope: :student, url: registers_path, local: true do |form| %>
      <div class="form-group">
        <strong><%= form.label :student_name %></strong><br>
        <%= form.text_field :student_name, class: 'form-control' %>
      </div>
      <div class="form-group">
      <strong><%= form.label :password %></strong><br>
      <%= form.password_field :password %>
      </div>
      <div class="form-group">
        <strong><%= form.label :student_id %></strong><br>
        <%= form.text_field :student_id, class: 'form-control' %>
      </div>
    <div>
    <strong><%= form.label :DOB %></strong><br>
    <%= form.date_field :dob %>
   </div>
<div>
<strong><%= form.label :choose_year %></strong><br>
<%= form.radio_button :choose_year, "first year" %>
<%= form.label :choose_year, "First year" %><br>
<%= form.radio_button :choose_year, "second year" %>
<%= form.label :choose_year, "Second year" %><br>
<%= form.radio_button :choose_year, "third year" %>
<%= form.label :choose_year, "Third year" %><br>
<%= form.radio_button :choose_year, "fourth year" %>
<%= form.label :choose_year, "Fourth year" %><br>
</div>
<div>
<strong><%= form.label :check_type %></strong><br>
<%= label_tag 'check_type_scholarship', 'scholarship'%>
<%= check_box_tag'student[check_type][]', 'scholarship', checked('scholarship'), id: 'check_type_scholarship'%><br>
<%= label_tag 'check_type_management quota', 'management quota'%>
<%= check_box_tag'student[check_type][]', 'management quota', checked('management quota'), id: 'check_type_management quota'%>
</div>
<div class="form-group">
<strong><%= form.label :select_branch %></strong><br>
<%= form.select :select_branch, ["CSE", "ECE", "EEE", "MECH", "IT", "CIVIL"], class: 'form-control' %>
</div>
<div class="form-group">
        <strong><%= form.label :address %></strong><br>
        <%= form.text_area :address, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= form.submit class: 'form-control btn-primary' %>
      </div>
    <% end %>